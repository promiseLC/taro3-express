{
  "extends": [
    "taro/react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "semi": "error",
    "no-console": "warn",
    "jsx-quotes": 0,
    "react/jsx-closing-bracket-location": "off",
    "--no-ignore": "off"
  }
}